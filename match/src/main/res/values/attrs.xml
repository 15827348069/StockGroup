<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomMyProgress">
        <attr name="cus_progress_current" format="integer"/>
        <attr name="cus_progress_max" format="integer"/>
        <attr name="cus_progress_unreached_color" format="color"/>
        <attr name="cus_progress_reached_start_color" format="color"/>
        <attr name="cus_progress_reached_color" format="color"/>
        <attr name="cus_progress_reached_end_color" format="color"/>
        <attr name="cus_progress_reached_bar_height" format="dimension"/>
        <attr name="cus_progress_unreached_bar_height" format="dimension"/>
        <attr name="cus_inner_drawable" format="reference"/>
        <attr name="cus_run_rate" format="enum">
            <enum name="fast" value="200"/>
            <enum name="medium" value="300"/>
            <enum name="slow" value="500"/>
        </attr>
    </declare-styleable>
    <!--魅族banner-->
    <declare-styleable name="MZBannerView">
        <attr name="open_mz_mode" format="boolean"/>
        <attr name="canLoop" format="boolean"/>
        <attr name="indicatorPaddingLeft" format="dimension"/>
        <attr name="indicatorPaddingRight" format="dimension"/>
        <attr name="indicatorPaddingTop" format="dimension"/>
        <attr name="indicatorPaddingBottom" format="dimension"/>
        <attr name="indicatorAlign">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="isShowIndicator" format="boolean"/>
        <attr name="middle_page_cover" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomMyCircleProgress">
        <attr name="custom_progress1" format="float"/>
        <attr name="progress_max1" format="integer"/>
        <attr name="unfinished_color1" format="color"/>
        <attr name="finished_color1" format="color"/>
        <attr name="finished_stroke_width1" format="dimension"/>
        <attr name="unfinished_stroke_width1" format="dimension"/>
        <attr name="text_size1" format="dimension"/>
        <attr name="text_color1" format="color"/>
        <attr name="progress_text_size1" format="dimension"/>
        <attr name="progress_text_color1" format="color"/>
        <attr name="background_color1" format="color"/>
        <attr name="circle_starting_degree1" format="integer" />
        <attr name="show_text1" format="boolean"/>
        <attr name="text1" format="string"/>
        <attr name="type1" format="string"/>
    </declare-styleable>
</resources>